  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild": "run-s format type-check lint:strict",
    "start": "next start",
    "format": "prettier --check --write \"{app,components,hooks,lib,src}/**/*.{js,ts,tsx}\" --log-level warn || (echo 'Formatting Failed' && exit 1)",
    "lint": "eslint \"{app,components,hooks,lib,src}/**/*.{js,ts,tsx}\" --format stylish",
    "lint:strict": "eslint \"{app,components,hooks,lib,src}/**/*.{ts,tsx}\" --max-warnings 0 --format codeframe --report-unused-disable-directives || (echo 'Linting Failed' && exit 1)",
    "build:ci": "run-s format type-check lint:strict build",
    "type-check": "tsc --noEmit --pretty --noErrorTruncation --skipLibCheck false || (echo 'Type Check Failed' && exit 1)",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "cypress run",
    "test:coverage": "jest --coverage",
    "audit": "pnpm audit",
    "deploy:staging": "vercel --token $VERCEL_TOKEN",
    "deploy:production": "vercel --prod --token $VERCEL_TOKEN",
    "docs": "typedoc",
    "preinstall": "node check-pnpm.js",
    "prepare": "husky"
  },
